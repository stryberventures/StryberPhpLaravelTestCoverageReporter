{
    "name": "stryber/laravel-test-coverage-reporter",
    "description": "Simple tool to send test coverage reports and generate badge",
    "license": "proprietary",
    "authors": [
        {
            "name": "Aleksandr Zhylinskiy",
            "email": "aleksandr@stryber.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "ext-simplexml": "*",
        "illuminate/support": "^8.0",
        "illuminate/config": "^8.0",
        "illuminate/console": "^8.0",
        "symfony/process": "^5.1.4",
        "phpunit/phpunit": "^9.3.3",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Stryber\\CoverageReporter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stryber\\CoverageReporter\\CoverageReporterServiceProvider"
            ]
        }
    }
}
